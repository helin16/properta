<com:TContent ID="MainContent">
	<section class="white-wrapper nopadding">
		<div id="map">
			<iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d12606.24717602874!2d144.96904466662528!3d-37.8237319178609!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sen!2sau!4v1415167861069" width="100%" height="400" frameborder="0" style="border:0"></iframe>
		</div>
	</section>
	<section class="white-wrapper nopadding contact-form-wrapper">
		<div class="container">
			<div class="section-title-heading margin-top">
				<h3 class="section-title">
					Leave <span class="font-light">Message</span>
				</h3>
			</div>
			<div class="contact-form">
				<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
					<input type="text" placeholder="Name" class="form-control"
						id="name" name="name"> <input type="text"
						placeholder="Email Address" class="form-control" id="email"
						name="email"> <input type="text" placeholder="Website"
						class="form-control" id="website" name="website">
				</div>
				<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
					<input type="text" placeholder="Subject" class="form-control" id="subject" name="subject">
					<textarea placeholder="Message" rows="6" id="comments" style="padding: 20px;height: 133px !important;" name="comments" class="form-control"></textarea>
					<span class="btn btn-lg btn-primary pull-right" id="submit" value="SEND" >Send Message</span>
				</div>
			</div>
			<!-- end contact-form -->
			<br> <br>
		</div>
		<!-- end container -->
	</section>
</com:TContent>