<com:TContent ID="MainContent">
<com:TCallback ID="getItemsBtn" onCallback="Page.getItems" />
<com:TCallback ID="updateMessageBtn" onCallback="Page.updateMessage" />
<div class="container  backend-div-wrapper">
	<div class="page-header">
		<div class="row">
			<div class="col-sm-8">
				<h3>Messages: <small id="totalNoOfItemsId">0</small></h3>
			</div>
			<div class="col-sm-4">
				<div class="input-group pull-right">
					<input type="search" class="form-control" placeholder="sender email ..." onkeydown="pageJs.keydown(event, function(){$('searchBtn').click();});">
					<div class="input-group-btn">
						<span class="btn btn-default" title="Search" id="searchBtn"><i class="glyphicon glyphicon-search"></i></span>
						<a id="new-property-btn" href="/backend/message/new.html" class="btn btn-success" title="Creat a Message" style="display: inline-block;"><i class="glyphicon glyphicon-plus"></i></a>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div id="result-div" class="property-list"></div>
</div>
</com:TContent>